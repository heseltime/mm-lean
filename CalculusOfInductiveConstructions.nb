Cell[BoxData[
 RowBox[{
  RowBox[{"(*", " ", 
   RowBox[{"Delta", "-", 
    RowBox[{
    "reduce", " ", "a", " ", "single", " ", "constant", " ", "of", " ", "the",
      " ", "environment"}]}], " ", "*)"}], "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{
    RowBox[{
     RowBox[{"DeltaReduce", "[", 
      RowBox[{"expr_", ",", "environment_List", ",", "constant_Symbol"}], 
      "]"}], ":=", 
     RowBox[{"Module", "[", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{"constants", ",", "defined"}], "}"}], ",", 
       "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"constants", "=", 
         RowBox[{"DefinedConstant", "[", "environment", "]"}]}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{"If", "[", 
         RowBox[{
          RowBox[{"FreeQ", "[", 
           RowBox[{"constants", ",", "constant"}], "]"}], ",", 
          RowBox[{"Return", "[", "expr", "]"}]}], "]"}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{"defined", "=", 
         RowBox[{"Extract", "[", 
          RowBox[{"environment", ",", 
           RowBox[{"First", "@", 
            RowBox[{"Position", "[", 
             RowBox[{"constants", ",", "constant"}], "]"}]}]}], "]"}]}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{"ReplaceRepeated", "[", 
         RowBox[{"expr", ",", 
          RowBox[{
           RowBox[{"makePattern", "[", 
            RowBox[{"First", "[", "defined", "]"}], "]"}], "\[Rule]", 
           RowBox[{"dropTyped", "[", 
            RowBox[{"Last", "[", "defined", "]"}], "]"}]}]}], "]"}]}]}], 
      "\[IndentingNewLine]", "]"}]}], ";"}], "\[IndentingNewLine]", 
   RowBox[{"(*", " ", 
    RowBox[{"Delta", "-", 
     RowBox[{"reduce", " ", "a", " ", "list", " ", "of", " ", "constants"}]}],
     " ", "*)"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{
     RowBox[{"DeltaReduce", "[", 
      RowBox[{"expr_", ",", "environment_List", ",", "list_List"}], "]"}], ":=", 
     RowBox[{"Fold", "[", 
      RowBox[{
       RowBox[{
        RowBox[{"DeltaReduce", "[", 
         RowBox[{"#1", ",", "environment", ",", "#2"}], "]"}], "&"}], ",", 
       "expr", ",", "list"}], "]"}]}], ";"}]}]}]], "Input",ExpressionUUID->\
"3ded7122-821f-4b7e-8c83-f1aab5cf9817"],

