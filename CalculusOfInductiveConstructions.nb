Cell["The elements of the language, all with special typesetting:", "Text",ExpressionUUID->"5621b315-3c9f-4124-bc80-b793cd2aea62"],

Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"Typed", "::", "usage"}], "=", 
   "\"\<Typed[e, t] declares that expression e has type t, usually typeset as \
e:t.\>\""}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{
    RowBox[{"ShowFrom", "::", "usage"}], "=", 
    "\"\<ShowFrom[type, term] is equivalent to Typed[term, type].\>\""}], 
   ";"}], "\[IndentingNewLine]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"LambdaFunction", "::", "usage"}], "=", 
   "\"\<LambdaFunction[v, e] represents the abstraction of the variable v \
over the term e, usually typeset as \[Lambda]v.e in untyped lambda calculus. \
LambdaFunction[Typed[v, t], e] represents the abstraction of the typed \
variable v, with type t, over the term e.\>\""}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{
    RowBox[{"Assume", "::", "usage"}], "=", 
    "\"\<Assume[assumptions, conclusion] is equivalent to \
LambdaFunction[assumptions, conclusion].\>\""}], ";"}], 
  "\[IndentingNewLine]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"ArrowType", "::", "usage"}], "=", 
   "\"\<ArrowType[t1, t2] is the type of functions mapping from type t1 to \
type t2, usually typeset as t1->t2.\>\""}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"PiType", "::", "usage"}], "=", 
   "\"\<PiType[Typed[v, t], tterm] is the dependent product type of functions \
mapping from type t to a type tterm dependent on the variable v of type \
t.\>\""}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"InductiveType", "::", "usage"}], "=", 
   "\"\<InductiveType[cons1, cons2, ...] represents an inductive type defined \
by the constructors consi.\>\""}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"Self", "::", "usage"}], "=", 
   "\"\<Self represents the type being defined inside an InductiveType[...] \
expression.\>\""}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"PropType", "::", "usage"}], "=", 
   "\"\<PropType, typeset as Prop, is the type of propositions. It evaluates \
to TypeU[0].\>\""}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"StarType", "::", "usage"}], "=", 
   "\"\<StarType, typeset as *, is the sort of all types. It evaluates to \
TypeU[1].\>\""}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"TypeU", "::", "usage"}], "=", 
   "\"\<TypeU[n] for a nonnegative integer n represents the universe of types \
of level n. Level 0 corresponds to the impredicative universe of \
propositions.\>\""}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"Pair", "::", "usage"}], "=", 
   "\"\<Pair[e1, e2] represents a pair of terms e1, e2. It is equivalent to \
Tuple[e1, e2].\>\""}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"Tuple", "::", "usage"}], "=", 
   "\"\<Tuple[e1, e2, e3, ...] represents a tuple of terms e1, e2, e3, \
...\>\""}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"ProductType", "::", "usage"}], "=", 
   "\"\<ProductType[t1, t2, t3, ...] is a type representing the independent \
product of types t1, t2, t3, ...\>\""}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"SigmaType", "::", "usage"}], "=", 
   "\"\<SigmaType[Typed[v, t], tterm] is the dependent sum type of Pair[v, \
term[v]], where v has type t and term[v] has a type tterm, still depending on \
v.\>\""}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"Defined", "::", "usage"}], "=", 
   "\"\<Defined[def, term] defines the definiendum def to be the definiens \
term.\>\""}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"NullDefiniens", "::", "usage"}], 
   "\"\<NullDefiniens is a symbol that represents the definiens in a \
primitive definition.\>\""}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"Math", "::", "usage"}], "=", 
   "\"\<Math[\[CapitalDelta], \[CapitalGamma], st] represents the \
mathematical statement st in the typing context \[CapitalGamma] and the \
environment \[CapitalDelta] of definitions. The statement can be a typing \
judgement with head Type or a definition with head Defined. Math[\
\[CapitalGamma], st] assumes an empty environment. Math[st] assumes an empty \
environment and an empty context.\>\""}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"DerivationRule", "::", "usage"}], "=", 
   "\"\<DerivationRule[{prem1, prem2, ...}, conc] represents the \
Gentzen-style derivation rule from the premises premi to the conclusion conc.\
\>\""}], ";"}]}], "Input",ExpressionUUID->"75d5355c-beed-4a52-9886-\
b38dba051db3"],

Cell["Manipulation of expressions:", "Text",ExpressionUUID->"6fb5f6fb-212b-438a-8d75-0fe4ee63af61"],

Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"RenameBound", "::", "usage"}], "=", 
   "\"\<RenameBound[e, v->w] renames the bound variable v to w in the \
expression e.\>\""}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"FreeVariables", "::", "usage"}], "=", 
   "\"\<FreeVariables[e] returns the list of all free variables (of any type \
or sort) in the expression e.\>\""}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"ReplaceFree", "::", "usage"}], "=", 
   "\"\<ReplaceFree[e, v->a] replaces the free variable v by the term a in \
expression e.\>\""}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"AlphaCanonical", "::", "usage"}], "=", 
   "\"\<AlphaCanonical[e] renames the bound variables in expression e using \
canonical names (formal variables), hence rendering a unique form of e, that \
can be compared to other expressions with SameQ to decide alpha-equivalence, \
as implemented in TildeEqual.\>\""}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"Subterms", "::", "usage"}], "=", 
   "\"\<Subterms[e] find the subterms of expression e.\>\""}], 
  ";"}]}], "Input",ExpressionUUID->"66875a62-433a-42b0-80e2-a980667a096e"],

Cell["Beta reduction:", "Text",ExpressionUUID->"1ea3842c-9223-49b2-9310-f91355299401"],

Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"BetaReduce", "::", "usage"}], "=", 
   "\"\<BetaReduce[e, v] beta-reduces the redex (possibly several) with bound \
