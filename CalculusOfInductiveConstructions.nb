  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"$TypeContext", "::", "usage"}], "=", 
   "\"\<$TypeContext is a global variable storing a list of global type \
declarations.\>\""}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"$TypeEnvironment", "::", "usage"}], "=", 
   "\"\<$TypeEnvironment is a global variable storing a list of global \
definitions..\>\""}], ";"}]}], "Input",ExpressionUUID->"19afc7ae-0f01-4269-\
b593-0095aea07d97"],

