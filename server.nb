  RowBox[{"HandleLeanServerResponse", "[", "p_ProcessObject", "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"msg", ",", "line", ",", "flag", ",", "res"}], "}"}], ",", 
    RowBox[{
     RowBox[{"flag", "=", "True"}], ";", 
     RowBox[{"msg", "=", "\"\<error\>\""}], ";", 
     RowBox[{"While", "[", 
      RowBox[{"flag", ",", 
       RowBox[{
        RowBox[{"line", "=", 
         RowBox[{"ImportString", "[", 
          RowBox[{
           RowBox[{"ReadLine", "[", "p", "]"}], ",", "\"\<JSON\>\""}], 
          "]"}]}], ";", 
        RowBox[{"If", "[", 
         RowBox[{
          RowBox[{
           RowBox[{"(", 
            RowBox[{"\"\<response\>\"", "/.", "line"}], ")"}], "\[Equal]", 
           "\"\<all_messages\>\""}], ",", 
          RowBox[{
           RowBox[{"flag", "=", "True"}], ";", 
           RowBox[{"msg", "=", 
            RowBox[{"(", 
             RowBox[{"\"\<msgs\>\"", "/.", "line"}], ")"}]}]}], ",", 
          RowBox[{
           RowBox[{"If", "[", 
            RowBox[{
             RowBox[{
              RowBox[{
               RowBox[{"(", 
                RowBox[{"\"\<response\>\"", "/.", "line"}], ")"}], 
               "\[Equal]", "\"\<ok\>\""}], "&&", 
              RowBox[{
               RowBox[{"(", 
                RowBox[{"\"\<message\>\"", "/.", "line"}], ")"}], "\[Equal]", 
               "\"\<file unchanged\>\""}]}], ",", 
             RowBox[{"msg", "=", "\"\<file unchanged\>\""}]}], "]"}], ";", 
           RowBox[{"flag", "=", "False"}]}]}], "]"}]}]}], "]"}], ";", 
